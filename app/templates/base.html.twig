<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>
        {% block title %}
            Tableau de bord | Republic republic of congo
        {% endblock %}
    </title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="stylesheet" type="text/css" href="{{ asset('media/fonts/fontawesome.css') }}">
    <link rel="stylesheet" href="{{ asset('media/css/libraries.css') }}"/>
    <link rel="stylesheet" href="{{ asset('media/css/app.css') }}"/>

    {% block javascripts %}
        <script>
            var mauticBasePath = '';
            var mauticBaseUrl = '/';
            var mauticAjaxUrl = '/s/ajax';
            var mauticAjaxCsrf = 'e02Y7xIQKrVZm0NSiEWWGQR5Ky1XIQfB9qNwGew-Gp0';
            var mauticAssetPrefix = '';
            var mauticContent = 'dashboard';
            var mauticEnv = 'prod';
            var mauticLang = {};
        </script>
        <script src="{{ asset('media/js/libraries.js') }}"></script>
        <script src="{{ asset('media/js/app.js') }}"></script>
        <script src="{{ asset('media/js/moment.min.js') }}"></script>
        <script src="{{ asset('media/js/chart.min.js') }}"></script>
        <script src="{{ asset('media/js/chart.esm.js') }}"></script>
        <script defer src="{{ asset('media/fonts/fontawesome-all.js') }}"></script>
        <script data-source="mautic">
            var mauticForms = {};
        </script>
    {% endblock %}


</head>
<body class="header-fixed">
<!-- start: app-wrapper -->
<section id="app-wrapper">

    <!-- start: app-sidebar(left) -->
        {{ include('shared/sidebar_left.html.twig') }}
    <!--/ end: app-sidebar(left) -->

    <!-- start: app-sidebar(right) -->
        {{ include('shared/sidebar_right.html.twig') }}
    <!--/ end: app-sidebar(right) -->

    <!-- start: app-header -->
    <header id="app-header" class="navbar">
        <!-- start: loading bar -->
        <div class="loading-bar">
            Chargement en cours...
        </div>
        <!--/ end: loading bar -->

        <!-- start: navbar nocollapse -->
        <div class="navbar-nocollapse">
            <!-- start: left nav -->
            <ul class="nav navbar-nav navbar-left">
                <li class="hidden-xs" data-toggle="tooltip" data-placement="right" title="Minimize Sidebar">
                    <a href="javascript:void(0)" data-toggle="minimize" class="sidebar-minimizer"><span
                                class="arrow fs-14"></span></a>
                </li>
                <li class="visible-xs">
                    <a href="javascript: void(0);" data-toggle="sidebar" data-direction="ltr">
                        <i class="fa fa-navicon fs-16"></i>
                    </a>
                </li>

                <li class="dropdown dropdown-custom" id="notificationsDropdown">
                    <a href="javascript: void(0);"
                       class="dropdown-toggle dropdown-notification" data-toggle="dropdown">
                        <span class="label label-danger" id="newNotificationIndicator"><i
                                    class="fa fa-asterisk"></i></span>
                        <span class="fa fa-bell fs-16"></span>
                    </a>
                    <div class="dropdown-menu">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <div class="panel-title">
                                </div>
                            </div>
                            <div class="pt-0 pb-xs pl-0 pr-0">
                                <div class="scroll-content slimscroll" style="height:250px;" id="notifications">
                                </div>
                            </div>
                        </div>
                    </div>
                </li>
            </ul>
            <!--/ end: left nav -->

            <!-- start: right nav -->
            <ul class="nav navbar-nav navbar-right">
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        <span class="img-wrapper img-rounded" style="width:32px;"><img
                                    src="https://www.gravatar.com/avatar/4dff60e91277adb92f869b6483afdfce?s=250&d=http%3A%2F%2Faeroport-nantes.xo-matic.com%2Fmedia%2Fimages%2Favatar.png"></span>
                        <span class="text fw-sb ml-xs hidden-xs">
                            {% if app.user is not empty %}
                                 {{ app.user.firstName }}
                            {%  endif %}
                        </span>
                        <span class="caret ml-xs"></span>
                    </a>
                    <ul class="dropdown-menu dropdown-menu-right">
                        <li>
                            <a href="{{ path('app_home') }}" data-toggle="ajax">
                                <i class="fa fa-user fs-14"></i><span>Compte</span>
                            </a>
                        </li>
                        <li>
                            <a href="{{ path('app_logout') }}">
                                <i class="fa fa-sign-out fs-14"></i><span>DÃ©connexion</span>
                            </a>
                        </li>

                    </ul>
                </li>
                <li>
                    <a href="javascript: void(0);" data-toggle="sidebar" data-direction="rtl">
                        <i class="fa fa-cog fs-16"></i>
                    </a>
                </li>
            </ul>
            <div class="navbar-toolbar pull-right mt-15 mr-10">
            </div>


            <!--/ end: right nav -->
        </div>
        <!--/ end: navbar nocollapse -->
        <div id="flashes" class="alert-growl-container">
        </div>
    </header>
    <!--/ end: app-header -->

    <!-- start: app-footer(need to put on top of #app-content)-->
    {{ include('shared/footer.html.twig') }}
    <!--/ end: app-content -->

    <!-- start: app-content -->
    {% block main %}
        <section id="app-content">

            <div class="content-body">

                <div class="page-header">
                    <div class="box-layout">
                        <div class="col-xs-5 col-sm-6 col-md-5 va-m">
                            <h3 class="pull-left">Tableau de bord</h3>
                            <div class="col-xs-2 text-right pull-left">
                            </div>
                        </div>
                        <div class="col-xs-7 col-sm-6 col-md-7 va-m">
                        </div>

                    </div>
                </div>
                <div class="row pt-md pl-md">
                    <div class="col-sm-6">

                    </div>
                </div>

            </div>

        </section>
    {% endblock %}
    <!--/ end: app-content -->

</section>
<!--/ end: app-wrapper -->
</body>
</html>
